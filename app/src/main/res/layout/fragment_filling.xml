<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Filling"
    android:background="@color/white">
    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="chocolate fill"
        android:textColor="#070707"
        android:textSize="40dp"
        android:background="@color/caramel_1"
        android:gravity="center"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Strawberry fill"
        android:layout_marginTop="70dp"
        android:textColor="@color/black"
        android:textSize="40dp"
        android:background="@color/caramel_1"
        android:gravity="center"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="140dp"
        android:background="@color/caramel_1"
        android:gravity="center"
        android:text="Banana fill"
        android:textColor="@color/black"
        android:textSize="40dp" />
    <TextView
        android:id="@+id/textView8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Coffee fill"
        android:layout_marginTop="210dp"
        android:textColor="@color/black"
        android:textSize="40dp"
        android:background="@color/caramel_1"
        android:gravity="center"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Confetti fill"
        android:layout_marginTop="280dp"
        android:textColor="@color/black"
        android:textSize="40dp"
        android:background="@color/caramel_1"
        android:gravity="center"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Tea fill"
        android:layout_marginTop="350dp"
        android:textColor="@color/black"
        android:textSize="40dp"
        android:background="@color/caramel_1"
        android:gravity="center"/>


</RelativeLayout>